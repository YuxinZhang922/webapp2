<h1>New Post</h1>

<form action="/posts" method="post">
  <%# add form fields %>
  
  <label for="author">Author</label>
  <input type="text" name="author" id="author">

  <label>Body</label>
  <input type="text" name="body"> 

  <label>Image URL</label>
  <input type="text" name="image">


  <button>Submit</button>
</form>
